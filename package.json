{
  "name": "ide-haskell",
  "main": "./lib/ide-haskell",
  "version": "1.6.5",
  "description": "Haskell IDE",
  "keywords": [
    "ide-haskell",
    "ide",
    "haskell"
  ],
  "repository": "https://github.com/atom-haskell/ide-haskell",
  "license": "MIT",
  "activationCommands": {
    "atom-workspace": "ide-haskell:open-settings"
  },
  "activationHooks": [
    "language-haskell:grammar-used"
  ],
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "providedServices": {
    "ide-haskell-upi": {
      "description": "Universal pluggable interface",
      "versions": {
        "0.0.1": "provideUpi"
      }
    }
  },
  "dependencies": {
    "temp": "^0.8.1",
    "sub-atom": "^1.0.0",
    "underscore-plus": "^1.6.6",
    "atom-space-pen-views": "^2.0.2",
    "atom-haskell-utils": "^0.7.8"
  }
}
